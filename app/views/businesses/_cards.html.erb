<div class="business-cards">
  <% if business.photo.present? %>
    <div class="business-image" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url(<%= cl_image_path business.photo %>)">
      <h3><%= business.title %></h3>
    </div>
  <% else %>
    <div class="business-image" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url(<%= image_url('logo_moh.png') %>)">
      <h3><%= business.title %></h3>
    </div>
  <% end %>
</div>
