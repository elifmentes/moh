<div class="container">
  <div class="card-container">
    <div class="row">
      <div class="cards">
        <% @categories.each do |category| %>
          <%#x= link_to category_businesses_path(category) do %>
            <%= render 'card', category: category %>
          <%# end %>
        <% end %>
      </div>
    </div>
  </div>
</div>
